{
  "name": "althea-dash",
  "main": "electron.js",
  "productName": "althea",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "start": "PORT=3005 react-scripts start",
    "start-prod": "cross-env NODE_ENV=production electron electron.js",
    "build": "react-scripts build && rm build/static/**/*.map && cp electron.js build",
    "package-android": "cordova build android --release",
    "package-ios": "cordova build ios --release --buildFlag=\"-useModernBuildSystem=0\"",
    "package-linux": "electron-packager build althea --platform=linux --arch=x64 --icon=public/icons/png/1024x1024.png --prune=true",
    "package-mac": "electron-packager build althea --osx-sign.identity=\"Mac Developer: Adam Soltys (256XBSY5HB)\" --overwrite --platform=darwin --arch=x64 --icon=public/icons/mac/althea.icns --prune=true",
    "package-windows": "electron-packager build althea --overwrite --platform=win32 --icon=public/icons/win/althea.ico --version-string.CompanyName=Althea --version-string.FileDescription=Althea --version-string.ProductName=\"Althea\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": ".",
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "ajv": "^6.5.2",
    "animated-scroll-to": "^1.2.2",
    "babel-eslint": "9",
    "bignumber.js": "^7.2.1",
    "bootstrap": "^4.1.3",
    "camelcase-keys-deep": "0.1.0",
    "concurrently": "^4.0.1",
    "cordova": "^8.1.2",
    "cordova-android": "^7.1.1",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-qrscanner": "git+https://github.com/asoltys/cordova-plugin-qrscanner.git",
    "cordova-plugin-whitelist": "1",
    "core-util-is": "^1.0.2",
    "cross-env": "^5.2.0",
    "electron": "^3.0.13",
    "electron-builder": "^20.29.0",
    "electron-is-dev": "^1.0.1",
    "electron-packager": "^13.0.1",
    "eslint": "5.6.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.12.4",
    "i": "^0.3.6",
    "i18next": "^14.0.1",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "ip-address": "^5.8.9",
    "jquery": "^1.12.4",
    "node-sass": "^4.11.0",
    "npm": "^6.3.0",
    "popper.js": "^1.14.4",
    "prettier": "1.14.0",
    "prop-types": "^15.6.2",
    "qrcode.react": "^0.8.0",
    "qrious": "^4.0.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-i18next": "^10.0.0",
    "react-qr-reader": "^2.1.1",
    "react-scripts": "^2.1.3",
    "react-sigma": "^1.2.30",
    "react-stateful": "https://github.com/asoltys/react-stateful",
    "reactstrap": "^7.0.2",
    "styled-components": "^3.4.9",
    "unstated": "^2.1.1",
    "web3": "^1.0.0-beta.35"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-add-swift-support": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-qrscanner": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "email-validator": "^2.0.4",
    "react-copy-to-clipboard": "^5.0.1"
  }
}
