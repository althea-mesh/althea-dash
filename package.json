{
  "name": "althea-dash",
  "main": "electron.js",
  "productName": "althea",
  "version": "0.1.6",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "ajv": "^6.5.2",
    "animated-scroll-to": "^1.2.2",
    "babel-eslint": "^7.2.3",
    "bignumber.js": "^7.2.1",
    "bootstrap": "^4.1.3",
    "camelcase-keys-deep": "0.1.0",
    "core-util-is": "^1.0.2",
    "cross-env": "^5.2.0",
    "debounce": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "i": "^0.3.6",
    "i18next": "^11.7.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "ip-address": "^5.8.9",
    "jquery": "^1.12.4",
    "npm": "^6.3.0",
    "popper.js": "^1.14.4",
    "qrcode.react": "^0.8.0",
    "qrious": "^4.0.2",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-i18next": "6.0.6",
    "react-qr-reader": "^2.1.1",
    "react-scripts": "1.1.1",
    "react-sigma": "^1.2.30",
    "react-stateful": "git+https://github.com/jtremback/react-stateful.git",
    "reactstrap": "^5.0.0-beta",
    "styled-components": "^3.4.9",
    "web3": "^1.0.0-beta.35"
  },
  "scripts": {
    "start": "PORT=3005 react-scripts start",
    "start-prod": "cross-env NODE_ENV=production electron electron.js",
    "build": "react-scripts build && rm build/static/**/*.map",
    "package-android": "cordova build android",
    "package-ios": "cordova build ios --release --buildFlag=\"-useModernBuildSystem=0\"",
    "package-linux": "electron-packager . Althea --platform=linux --arch=x64 --icon=public/icons/png/1024x1024.png --prune=true",
    "package-mac": "electron-packager . Althea --osx-sign.identity=\"Mac Developer: Adam Soltys (256XBSY5HB)\" --overwrite --platform=darwin --arch=x64 --icon=public/icons/mac/althea.icns --prune=true",
    "package-windows": "electron-packager . Althea --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=Althea --version-string.FileDescription=Althea --version-string.ProductName=\"Althea\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": ".",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "cordova": "^8.1.2",
    "cordova-android": "^7.1.1",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-qrscanner": "git+https://github.com/asoltys/cordova-plugin-qrscanner.git",
    "cordova-plugin-whitelist": "1",
    "electron": "^3.0.6",
    "electron-builder": "^20.29.0",
    "electron-is-dev": "^1.0.1",
    "electron-packager": "^12.2.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "1.14.0",
    "wait-on": "^3.1.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-add-swift-support": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-qrscanner": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
